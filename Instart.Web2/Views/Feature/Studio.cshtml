@using Instart.Models
@model Studio

@{
    ViewBag.Title = "ProToPro";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!--banner展示图-->
@Html.Partial("PartialBanner", Instart.Models.Enums.EnumBannerPos.About)

<div class="maincont">
    <!--特色菜单-->
    @Html.Partial("PartialFeature", new KeyValuePair<string, string>("type", ViewBag.Type.ToString()))

    <div class="maincont padding30 overflow">
        <div class="xue-left fl">
            <div class="course-item">
                <a href="#" class="cimg"><img src="@Model.ImgUrl" alt="" /></a>
                <div class="c-guide">
                    <p> <b>24 小时工作室</b> <span>24 Hour Lab</span> </p>
                </div>
                <div class="c-infos">@Model.Introduce</div>
            </div>
        </div>
        <div class="xue-right fr">
            <div class="claimlist">
                @Model.Details
            </div>
        </div>
    </div>
</div>

<!--实习单位环境/作品 轮播图-->
<div class="workbo">
    <div class="bd">
        <ul>
            <li> <a href="#"><img src="../yishaimg/lingshi/dabaner.jpg" /></a> </li>
            <li> <a href="#"><img src="../yishaimg/lingshi/dabaner.jpg" /></a> </li>
        </ul>
    </div>
    <div class="hd"> <a class="prev"></a> <a class="next"></a> </div>
</div>

<div class="maincont">
    <!--课程推荐-->
    @Html.Partial("PartialRecommendCourse")
</div>

